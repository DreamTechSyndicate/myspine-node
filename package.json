{
  "name": "myspine-node",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/src/server.js",
  "license": "MIT",
  "dependencies": {
    "argon2": "^0.40.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc && find . -xdev -path 'src/db/{migrations,seeds}' -name '*.js' -type f -exec sh -c 'mv \"$0\" \"${0%.js}.cjs\"' {} \\;",
    "start": "nodemon --experimental-specifier-resolution=node",
    "dev": "yarn build && yarn start",
    "lint": "yarn eslint --ext .js,.ts --fix . && yarn prettier --write .",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.19",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "ajv": "^8.12.0",
    "better-sqlite3-session-store": "^0.1.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "http-terminator": "^3.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "typescript": "^5.3.3"
  }
}
